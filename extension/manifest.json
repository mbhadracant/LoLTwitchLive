{
    "name": "TwitchLoLPlayersInfo",
    "description": "Shows information on all players in the live game the LoL streamer is playing",
    "version": "0.1",
    "permissions": [
    "tabs","<all_urls>"
    ],
    "content_scripts": [
        {
        "matches": [ "*://*.twitch.tv/*"],
        "js": ["jquery.js", "bootstrap.js", "script.js"],
        "css": ["bootstrap.css", "main.css"],
        "run_at": "document_end"
        }
    ],
    "manifest_version":2
}
